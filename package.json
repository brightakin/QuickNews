{
  "name": "lendsqr",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "codepush:ios": "appcenter codepush release-react -a akinkuotubright-gmail.com/Lendsqr-ios -d Production -m --description 'changes made'",
    "codepush:android": "appcenter codepush release-react -a akinkuotubright-gmail.com/Lendsqr-android -d Production -m --description 'changes made'",
    "build:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "husky:prettier": "npx husky add .husky/pre-commit 'yarn run prettier'",
    "husky:setup": "npx husky-init && yarn run husky:commitlint && yarn run husky:prettier",
    "prepare": "husky install",
    "delete": "cd android && rm -rf app/src/main/res/drawable* && rm -rf app/src/main/res/raw/*.json && cd .. ",
    "postinstall": "react-native setup-ios-permissions && pod-install"
  },
  "dependencies": {
    "@notifee/react-native": "^7.8.2",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-native-community/hooks": "^3.0.0",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/netinfo": "^11.3.2",
    "@react-native-firebase/analytics": "^20.1.0",
    "@react-native-firebase/app": "^20.1.0",
    "@react-native-firebase/crashlytics": "^20.1.0",
    "@react-native-firebase/messaging": "^20.1.0",
    "@react-native-firebase/perf": "^20.1.0",
    "@react-native-firebase/remote-config": "^20.1.0",
    "@react-native-google-signin/google-signin": "^12.2.1",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/stack": "^6.3.29",
    "@reduxjs/toolkit": "^2.2.5",
    "axios": "^1.7.2",
    "formik": "^2.4.6",
    "moment": "^2.30.1",
    "react": "18.2.0",
    "react-native": "0.74.2",
    "react-native-asset": "^2.1.1",
    "react-native-code-push": "^8.2.2",
    "react-native-gesture-handler": "^2.17.1",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-logs": "^5.1.0",
    "react-native-paper": "^5.12.3",
    "react-native-reanimated": "^3.12.1",
    "react-native-responsive-screen": "^1.4.2",
    "react-native-safe-area-context": "^4.10.5",
    "react-native-screens": "^3.32.0",
    "react-native-svg": "^15.3.0",
    "react-native-vector-icons": "^10.1.0",
    "react-redux": "7.2.2",
    "redux": "4.0.5",
    "redux-mock-store": "^1.5.4",
    "redux-persist": "^6.0.0",
    "redux-thunk": "2.3.0",
    "styled-components": "^6.1.11",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@babel/runtime": "^7.20.0",
    "@jest/globals": "^29.7.0",
    "@react-native/babel-preset": "0.74.84",
    "@react-native/eslint-config": "0.74.84",
    "@react-native/metro-config": "0.74.84",
    "@react-native/typescript-config": "0.74.84",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.5.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-native": "^0.73.0",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-redux": "^7.1.33",
    "@types/react-test-renderer": "^18.0.0",
    "@types/redux-mock-store": "^1.0.6",
    "babel-jest": "^29.7.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "2.8.8",
    "react-native-svg-transformer": "^1.4.0",
    "react-test-renderer": "18.2.0",
    "ts-jest": "^29.1.5",
    "typescript": "5.0.4"
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!@react-native|react-native)"
    ],
    "setupFiles": [
      "./node_modules/@react-native-google-signin/google-signin"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "engines": {
    "node": ">=18"
  }
